<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="TitleBar">
        <attr name="title" format="string" />
        <attr name="actionName" format="string" />
        <attr name="enableHelp" format="boolean" />
        <attr name="enableAction" format="boolean" />
        <attr name="actionStyle" format="enum">
            <enum name="text" value="1" />
            <enum name="button" value="2" />
        </attr>
        <attr name="hideDivider" format="boolean" />
    </declare-styleable>
</resources>